@model session1.Models.User

@{
    ViewData["Title"] = "PrintDetails";
}

<h1>Print Details</h1>

<div>
    <h4>User</h4>
    <hr />
    <dl class="row">
        
        <dt class = "col-sm-2">
           Name 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class = "col-sm-2">
           Age
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserAge)
        </dd>
        <dt class = "col-sm-2">
           Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
    </dl>
</div>
